@charset "utf-8";
/* CSS Document */


.nav > li > a:hover,
.nav > li > a:focus {
  text-decoration: none;
  background-color:inherit;
}
.navbar-content .dropdown-toggle{ cursor:pointer;}
.navbar-content .dropdown .dropdown-menu{ right:0px; left:inherit; margin-top:10px; text-align:right; min-width:100px;
	-webkit-box-shadow: none;
     -moz-box-shadow: none;
          box-shadow: none;
}
.navbar-content .nav  .dropdown .dropdown-menu li{ float:none;}
.navbar-content .nav{ margin:0px;}

.btn-group-00acac .btn-primary{
	color: #ffffff;
  text-shadow: none;
  box-shadow:none;
  background-color: #00acac;
  *background-color: #00acac;
  background-image: -moz-linear-gradient(top, #00acac, #00acac);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#00acac), to(#00acac));
  background-image: -webkit-linear-gradient(top, #00acac, #00acac);
  background-image: -o-linear-gradient(top, #00acac, #00acac);
  background-image: linear-gradient(to bottom, #00acac, #00acac);
  background-repeat: repeat-x;
  border-color: #00acac #00acac #00acac;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00acac', endColorstr='#00acac', GradientType=0);
  filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
  padding:5px 6px 5px 12px;
  font-size:12px;
  font-family:'宋体';
	
}
.btn-group.btn-group-00acac > .btn + .dropdown-toggle{  box-shadow:none; border-left:1px solid #029797; z-index:3;}
.btn-group.btn-group-00acac.open .btn-primary.dropdown-toggle{ background:#00acac;}
.btn-group-00acac{ margin:8px 7px 0 0;}
.btn-menu .btn-group-00acac li{ float:none;}
.btn-group-00acac .dropdown-menu{ min-width:117px; border-radius:3px;}
.btn-group-00acac .dropdown-menu > li > a{ padding:4px 5px 4px 17px; color: #767676;}


.dropdown-menu > li > a:hover,
.dropdown-menu > li > a:focus,
.dropdown-submenu:hover > a,
.dropdown-submenu:focus > a {
  color: #767676;
  text-decoration: none;
  background-color: #edf0f5;
  background-image: -moz-linear-gradient(top, #edf0f5, #edf0f5);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#edf0f5), to(#edf0f5));
  background-image: -webkit-linear-gradient(top, #edf0f5, #edf0f5);
  background-image: -o-linear-gradient(top, #edf0f5, #edf0f5);
  background-image: linear-gradient(to bottom, #edf0f5, #edf0f5);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#edf0f5', endColorstr='#edf0f5', GradientType=0);
}

.btn-group-fff{ background:#fff; border:1px solid #ced5dc; border-radius:3px; margin:8px 0 0 8px;}
.btn-group-fff a.dropdown-toggle{ color:#657691; padding:3px 8px 3px 22px; height:24px; line-height:24px; font-size:12px; color:#657691; font-family:'宋体'; display:inline-block; box-shadow:none;}
.btn-group-fff a.dropdown-toggle:hover{ text-decoration:none; box-shadow:none;}
.btn-group.btn-group-fff.open .dropdown-toggle{ box-shadow:none; text-decoration:none;}
.btn-group-fff a.dropdown-toggle .caret{ vertical-align:middle; margin:0px 4px 0px 16px;}
.btn-group-fff .dropdown-menu{ min-width:116px;}
.btn-group-fff .dropdown-menu li{ float:none;}
.dropdown-menu li a{ font-size:12px; padding:4px 5px 4px 17px; color:#767676;}
.lb-main .table td{ padding:5px 8px; border-top:0px none; border-bottom:1px solid #eaf0f7;}
.lb-main .table thead:first-child tr:first-child th{ font-size:12px; color:#888888; font-family:'宋体'; background:#fafafa; border-top:1px solid #e8e8e8; border-bottom:1px solid #e8e8e8; padding:10px 8px;}
.lb-main .table thead:first-child tr:first-child th .caret{ border-bottom-color:#7f7f7f; border-top-color:#7f7f7f; vertical-align:middle;}

.lb-main .table thead:first-child tr:first-child th .dropdown .caret{ margin:0px;}
/*-----------------select---------------*/
.bootstrap-select .btn{ background:#fff; border:1px solid #ced5dc; border-radius:3px; color:#657691;}
.bootstrap-select .btn span.filter-option{ color:#657691;}
.bootstrap-select.btn-group.open .btn.dropdown-toggle{ background:#fff; border:1px solid #dddddd; border-radius:3px; box-shadow:none; color:#657691;}
.bootstrap-select.btn-group > .btn:first-child{ border-radius:3px;}
.bootstrap-select .dropdown-menu{ border-radius:3px;}

.bootstrap-select .dropdown-menu > li > a:hover,
.bootstrap-select .dropdown-menu > li > a:focus,
.bootstrap-select .dropdown-submenu:hover > a,
.bootstrap-select .dropdown-submenu:focus > a {
  color: #ffffff;
  text-decoration: none;
  background-color: #edf0f5;
  background-image: -moz-linear-gradient(top, #edf0f5, #edf0f5);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#edf0f5), to(#edf0f5));
  background-image: -webkit-linear-gradient(top, #edf0f5, #edf0f5);
  background-image: -o-linear-gradient(top, #edf0f5, #edf0f5);
  background-image: linear-gradient(to bottom, #edf0f5, #edf0f5);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#edf0f5', endColorstr='#edf0f5', GradientType=0);
}
.bootstrap-select{ width:auto;}
select.selectpicker{ width:auto;}

.bootstrap-select:not([class*="col-"]):not([class*="form-control"]):not(.input-group-btn) {
	width:100%;
}
.xuanz-right .bootstrap-select li{ float:none; font-size:12px; color:#657691; font-family:'宋体';}
.bootstrap-select.btn-group .dropdown-menu li a span.text{ color:#767676;}

/*-----------------breadcrumb---------------*/
.page-breadcrumb{ margin:20px 0px 0px 19px;}
.breadcrumb {
  padding: 0;
  margin: 0 0 20px;
  background-color: #fff; font-size:12px; color:#7c87a5; font-family:'宋体';
}
.breadcrumb > li > .divider{ color:#c8c6c6; font-family:'宋体';}
.breadcrumb > .active{ color:#7c87a5;}
.breadcrumb a{ color:#3083eb;}
/*-----------------滚动条---------------*/
.mCSB_container{ margin-right:0px;}
.mCustomScrollBox>.mCSB_scrollTools{ width:13px;}
.mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar{ width:7px;}
.mCS-dark > .mCSB_scrollTools .mCSB_draggerRail{ background:none;}
.mCS-dark > .mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar{ background:#d1d1d1;}
.mCS-dark>.mCSB_scrollTools .mCSB_dragger:hover .mCSB_dragger_bar{ background:#91989f;}

/*-----------------分页---------------*/
.pagination{ text-align:center; margin-bottom:0px;}
.pagination ul {
	margin-top:0px;
  display: inline-block;
  *display: inline;
  margin-bottom: 0;
  margin-left: 0;
  -webkit-border-radius: 0px;
     -moz-border-radius: 0px;
          border-radius: 0px;
  *zoom: 1;
  -webkit-box-shadow: none;
     -moz-box-shadow: none;
          box-shadow: none;
}
.pagination ul > li > a,
.pagination ul > li > span {
  float: left;
  padding: 4px 12px;
  line-height: 20px;
  text-decoration: none;
  background-color: #ffffff;
  border: 1px solid #dbdbdb;
  border-left-width: 1px;
  color:#555056;
}
.pagination ul > li > a:hover,
.pagination ul > li > a:focus,
.pagination ul > .active > a,
.pagination ul > .active > span {
  background:#1abc9c; color:#fff;
}
.pagination ul > li > a, .pagination ul > li > span{ margin-right:10px;}
.pagination ul > li:first-child > a,
.pagination ul > li:first-child > span {
  border-left-width: 1px;
  -webkit-border-bottom-left-radius: 0px;
          border-bottom-left-radius: 0px;
  -webkit-border-top-left-radius: 0px;
          border-top-left-radius: 0px;
  -moz-border-radius-bottomleft: 0px;
  -moz-border-radius-topleft: 0px;
}

.pagination ul > li:last-child > a,
.pagination ul > li:last-child > span {
  -webkit-border-top-right-radius: 0px;
          border-top-right-radius: 0px;
  -webkit-border-bottom-right-radius: 0px;
          border-bottom-right-radius: 0px;
  -moz-border-radius-topright: 0px;
  -moz-border-radius-bottomright: 0px;
}
/*-----------------end分页---------------*/
/*-----------------Modal---------------*/
.page-modal{ border:0px none;}
.modal.fade.page-fixed-modal{ margin:0px 0px 120px -367px; width:735px; left:50%; border:0px none; box-shadow:none;}
.modal-header{ background:#2dcc70; padding:8px 18px 8px 25px; border:0px none;}
.modal-header .close{ margin-top:4px;}
.dialog-close{ background:url(../images/common/dialog_close.png) no-repeat scroll 0 0; background:rgba(25, 204, 112, 1) url(../images/common/dialog_close.png) no-repeat scroll 0 0; width:23px; height:23px; display:inline-block;}
.modal-header .close{ opacity:1; filter:alpha(opacity=100);}
.page-modal .modal-header h3{ font-size:20px; color:#fff; font-family:'微软雅黑'; font-weight:normal;}

.modal-footer{ height:65px; border-top:1px solid #e6e6e6; padding:0px 25px; border-radius:0px; background:#fff; box-shadow:none;}
.white-btn{ background:#ffffff; border:1px solid #cbcbcb; text-align:center; font-size:14px; line-height:14px; padding:11px 0px; width:122px; color:#333333; font-family:'微软雅黑'; border-radius:0px; margin-top:12px;}
.green-btn{ background:#2dcc70; border:1px solid #1bbd5f; text-align:center; font-size:14px; line-height:14px; padding:11px 0px; width:122px; color:#fff; font-family:'微软雅黑'; border-radius:0px; margin-top:12px; margin-right:20px; box-shadow:none;}

.green-btn:hover{ background:#2dcc70; opacity:0.8; filter:alpha(opacity=80);}




/*-----------------end Modal---------------*/
/*-----------------nav-tabs-25ae68---------------*/
.nav-tabs-25ae68.nav-tabs {
  border-bottom: 1px solid #25ae68;
}
.nav-tabs-25ae68.nav-tabs > li > a {
	color:#7f878d;
	font-size:14px;
	font-family:'微软雅黑';
  padding:10px 31px 11px 31px;
  line-height: 20px;
  border: 1px solid #e0e0e0;
  border-radius: 0;
  margin:0 0 0 -1px;
  border-bottom:1px solid #25ae68;
}
.nav-tabs-25ae68.nav-tabs > li > a:hover,
.nav-tabs-25ae68.nav-tabs > li > a:focus {
  border-color: #25ae68;
  background:#25ae68;
  color:#fff;
}
.nav-tabs-25ae68.nav-tabs > .active > a,
.nav-tabs-25ae68.nav-tabs > .active > a:hover,
.nav-tabs-25ae68.nav-tabs > .active > a:focus {
  color: #7f878d;
  cursor: default;
  background-color: #ffffff;
  border-width:3px 1px 0px 1px;
  border-color:#25ae68;
  border-style:solid;

  border-radius:0px;
  position:relative;
  z-index:2;
  padding:10px 31px 10px 31px;

}
/*-----------------end nav-tabs---------------*/
